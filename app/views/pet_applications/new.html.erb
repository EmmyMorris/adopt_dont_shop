<h1>New Application</h1>
<%= form_with url: "/pet_applications/new", method: :post, local: true do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
<br>
<br>
  <%= f.label :street_address %>
  <%= f.text_field :street_address %>
<br>
<br>
  <%= f.label :city %>
  <%= f.text_field :city %>
<br>
<br>
  <%= f.label :state %>
  <%= f.text_field :state %>
<br>
<br>
  <%= f.label :zip_code %>
  <%= f.text_field :zip_code %>
<br>
<br>
  <%= f.submit %>
<% end %>
